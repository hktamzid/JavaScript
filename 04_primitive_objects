//Non-Premitive data types.
const item = {  // These items are called objects
  "Tamzid": true,
  "Mahmud": false,
  "Angkon": 25,
  "Sompa": "wife", // string should be defined in quotation
  "Akter": undefined
}
console.log(" ")
console.log(item["Angkon"])
console.log(item["Sompa"])
console.log(item["Tamzid"])
